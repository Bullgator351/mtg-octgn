		<block type="overlay" id="bu" src="proxy/back/u.jpg">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bb" src="proxy/back/b.jpg">      <location x="0" y="0" />    </block>
		<block type="overlay" id="br" src="proxy/back/r.jpg">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bg" src="proxy/back/g.jpg">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bc" src="proxy/back/c.jpg">      <location x="0" y="0" />    </block>
		<block type="overlay" id="ba" src="proxy/back/a.jpg">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bl" src="proxy/back/l.jpg">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bm" src="proxy/back/m.jpg">      <location x="0" y="0" />    </block>

