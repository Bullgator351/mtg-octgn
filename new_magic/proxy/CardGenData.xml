		<block type="overlay" id="bhu" src="proxy/back/hu.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bhb" src="proxy/back/hb.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bhr" src="proxy/back/hr.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="bhg" src="proxy/back/hg.png">      <location x="0" y="0" />    </block>

		<block type="overlay" id="rw" src="proxy/rules/w.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="ru" src="proxy/rules/u.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rb" src="proxy/rules/b.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rr" src="proxy/rules/r.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rg" src="proxy/rules/g.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rc" src="proxy/rules/c.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rm" src="proxy/rules/m.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="ra" src="proxy/rules/a.png">      <location x="0" y="0" />    </block>

		<block type="overlay" id="rhw" src="proxy/rules/hw.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rhu" src="proxy/rules/hu.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rhb" src="proxy/rules/hb.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rhr" src="proxy/rules/hr.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="rhg" src="proxy/rules/hg.png">      <location x="0" y="0" />    </block>

		<block type="overlay" id="tw" src="proxy/type/w.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="tu" src="proxy/type/u.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="tb" src="proxy/type/b.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="tr" src="proxy/type/r.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="tg" src="proxy/type/g.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="tc" src="proxy/type/c.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="tm" src="proxy/type/m.png">      <location x="0" y="0" />    </block>
		<block type="overlay" id="ta" src="proxy/type/a.png">      <location x="0" y="0" />    </block>

		<block type="text" id="title">
			<location x="31" y="32" />
			<text color="#000000" size="17" font="fonts/mplantin_symbols.ttf" />
			<border color="#000000" size="1" />
			<wordwrap height="18" width="318" />
		</block>
		<block type="text" id="cost">
			<location x="31" y="32" />
			<text color="#000000" size="17" font="fonts/mplantin_symbols.ttf" />
			<border color="#000000" size="1" />
			<wordwrap height="18" width="316" align="far" />
		</block>
		<block type="text" id="rarity">
			<location x="31" y="160" />
			<text color="#000000" size="17" font="fonts/mplantin_symbols.ttf" />
			<border color="#FFFFFF" size="2" />
			<wordwrap height="18" width="316" align="far" />
		</block>
		<block type="text" id="type">
			<location x="31" y="205" />
			<text color="#000000" size="17" font="fonts/mplantin_symbols.ttf" />
			<border color="#000000" size="1" />
			<wordwrap height="18" width="318" />
		</block>
		<block type="text" id="rules">
			<location x="37" y="240" />
			<text color="#000000" size="15" font="fonts/mplantin_symbols.ttf" />
			<wordwrap height="227" width="301" />
		</block>
		<block type="text" id="ptbox">
			<location x="143" y="474" />
			<text color="#000000" size="27" font="fonts/mplantin_symbols.ttf" />
			<border color="#FFFFFF" size="3" />
			<wordwrap height="18" width="200" align="far" />
		</block>
	</blocks>

	<template src="proxy/back/c.jpg" default="True" >
		<matches>
			<match name="Flags" value=""/>
		</matches>
		<overlayblocks>
			<link block="bc" />
			<conditional>
				<switch property="Color">
					<case contains="Multicolor" ><link block="bm" /></case>
					<case value="White"><link block="bw" /></case>
					<case value="Blue"><link block="bu" /></case>
					<case value="Black"><link block="bb" /></case>
					<case value="Red"><link block="br" /></case>
					<case value="Green"><link block="bg" /></case>
					<case value="Hybrid White Blue"><link block="bw" /><link block="bhu" /></case>
					<case value="Hybrid White Black"><link block="bw" /><link block="bhb" /></case>
					<case value="Hybrid Blue Black"><link block="bu" /><link block="bhb" /></case>
					<case value="Hybrid Blue Red"><link block="bu" /><link block="bhr" /></case>
					<case value="Hybrid Black Red"><link block="bb" /><link block="bhr" /></case>
					<case value="Hybrid Black Green"><link block="bb" /><link block="bhg" /></case>
					<case value="Hybrid Red Green"><link block="br" /><link block="bhg" /></case>
					<case value="Hybrid Red White"><link block="br" /><link block="bhw" /></case>
					<case value="Hybrid Green White"><link block="bg" /><link block="bhw" /></case>
					<case value="Hybrid Green Blue"><link block="bg" /><link block="bhu" /></case>
				</switch>
			</conditional>
			<conditional>
				<switch property="Type">
					<case contains="Land" ><link block="bl" /><link block="rc" /><link block="tc" /></case>
					<case contains="Artifact" break="True"><link block="ba" /><link block="ra" /><link block="ta" /></case>
				</switch>
			</conditional>
			<conditional>
				<switch property="Flags">
					<case value="W" ><link block="rw" /><link block="tw" /></case>
					<case value="U"><link block="ru" /><link block="tu" /></case>
					<case value="B"><link block="rb" /><link block="tb" /></case>
					<case value="R"><link block="rr" /><link block="tr" /></case>
					<case value="G"><link block="rg" /><link block="tg" /></case>
					<case value="M"><link block="rm" /><link block="tm" /></case>
					<case value="C"><link block="rc" /><link block="tc" /></case>
					<case value="WU"><link block="tm" /><link block="rw" /><link block="rhu" /></case>
					<case value="WB"><link block="tm" /><link block="rw" /><link block="rhb" /></case>
					<case value="UB"><link block="tm" /><link block="ru" /><link block="rhb" /></case>
					<case value="UR"><link block="tm" /><link block="ru" /><link block="rhr" /></case>
					<case value="BR"><link block="tm" /><link block="rb" /><link block="rhr" /></case>
					<case value="BG"><link block="tm" /><link block="rb" /><link block="rhg" /></case>
					<case value="RG"><link block="tm" /><link block="rr" /><link block="rhg" /></case>
					<case value="RW"><link block="tm" /><link block="rr" /><link block="rhw" /></case>
					<case value="GW"><link block="tm" /><link block="rg" /><link block="rhw" /></case>
					<case value="GU"><link block="tm" /><link block="rg" /><link block="rhu" /></case>
				</switch>		
			</conditional>
			<conditional>
				<switch property="Color">
					<case value="White" ><link block="rw" /><link block="tw" /></case>
					<case value="Blue"><link block="ru" /><link block="tu" /></case>
					<case value="Black"><link block="rb" /><link block="tb" /></case>
					<case value="Red"><link block="rr" /><link block="tr" /></case>
					<case value="Green"><link block="rg" /><link block="tg" /></case>
					<case contains="Multicolor" break='False'><link block="tm" /></case>
					<case contains="Hybrid" break='False'><link block="tc" /></case>
					<case value="Hybrid White Blue"><link block="rw" /><link block="rhu" /></case>
					<case value="Hybrid White Black"><link block="rw" /><link block="rhb" /></case>
					<case value="Hybrid Blue Black"><link block="ru" /><link block="rhb" /></case>
					<case value="Hybrid Blue Red"><link block="ru" /><link block="rhr" /></case>
					<case value="Hybrid Black Red"><link block="rb" /><link block="rhr" /></case>
					<case value="Hybrid Black Green"><link block="rb" /><link block="rhg" /></case>
					<case value="Hybrid Red Green"><link block="rr" /><link block="rhg" /></case>
					<case value="Hybrid Red White"><link block="rr" /><link block="rhw" /></case>
					<case value="Hybrid Green White"><link block="rg" /><link block="rhw" /></case>
					<case value="Hybrid Green Blue"><link block="rg" /><link block="rhu" /></case>
					<case value="Multicolor White Blue"><link block="rw" /><link block="rhu" /></case>
					<case value="Multicolor White Black"><link block="rw" /><link block="rhb" /></case>
					<case value="Multicolor Blue Black"><link block="ru" /><link block="rhb" /></case>
					<case value="Multicolor Blue Red"><link block="ru" /><link block="rhr" /></case>
					<case value="Multicolor Black Red"><link block="rb" /><link block="rhr" /></case>
					<case value="Multicolor Black Green"><link block="rb" /><link block="rhg" /></case>
					<case value="Multicolor Red Green"><link block="rr" /><link block="rhg" /></case>
					<case value="Multicolor Red White"><link block="rr" /><link block="rhw" /></case>
					<case value="Multicolor Green White"><link block="rg" /><link block="rhw" /></case>
					<case value="Multicolor Green Blue"><link block="rg" /><link block="rhu" /></case>
					<case contains="Multicolor"><link block="rm" /></case>
					<case contains="Hybrid"><link block="rc" /></case>
				</switch>
			</conditional>
		<link block="blank" />
		</overlayblocks>
		<textblocks>
			<link block="title"><property name="Name" /></link>
			<link block="cost"><property name="Cost" /></link>
			<link block="rarity"><property name="Rarity" /></link>
			<link block="type" separator=" --  "><property name="Type" /><property name="Subtype" /></link>
			<link block="rules"><property name="Rules" /></link>
			<link block="ptbox"><property name="PT Box" /></link>
		</textblocks>
	</template>
</templates>
