	<template src="proxy/back/l.jpg" default="True" >
		<matches>
		</matches>
		<overlayblocks>
			<link block="bl" />
			<conditional><if property="Flags" value="W"><link block="rw" /><link block="tw" /></if></conditional>
			<conditional><if property="Flags" value="U"><link block="ru" /><link block="tu" /></if></conditional>
			<conditional><if property="Flags" value="B"><link block="rb" /><link block="tb" /></if></conditional>
			<conditional><if property="Flags" value="R"><link block="rr" /><link block="tr" /></if></conditional>
			<conditional><if property="Flags" value="G"><link block="rg" /><link block="tg" /></if></conditional>
			<conditional><if property="Flags" value="M"><link block="rm" /><link block="tm" /></if></conditional>
			<conditional><if property="Flags" value="C"><link block="rc" /><link block="tc" /></if></conditional>
			<conditional><if property="Flags" value="WU"><link block="tm" /><link block="rw" /><link block="rhu" /></if></conditional>
			<conditional><if property="Flags" value="WB"><link block="tm" /><link block="rw" /><link block="rhb" /></if></conditional>
			<conditional><if property="Flags" value="UB"><link block="tm" /><link block="ru" /><link block="rhb" /></if></conditional>
			<conditional><if property="Flags" value="UR"><link block="tm" /><link block="ru" /><link block="rhr" /></if></conditional>
			<conditional><if property="Flags" value="BR"><link block="tm" /><link block="rb" /><link block="rhr" /></if></conditional>
			<conditional><if property="Flags" value="BG"><link block="tm" /><link block="rb" /><link block="rhg" /></if></conditional>
			<conditional><if property="Flags" value="RG"><link block="tm" /><link block="rr" /><link block="rhg" /></if></conditional>
			<conditional><if property="Flags" value="RW"><link block="tm" /><link block="rr" /><link block="rhw" /></if></conditional>
			<conditional><if property="Flags" value="GW"><link block="tm" /><link block="rg" /><link block="rhw" /></if></conditional>
			<conditional><if property="Flags" value="GU"><link block="tm" /><link block="rg" /><link block="rhu" /></if></conditional>
		</overlayblocks>
		<textblocks>
			<link block="title"><property name="Name" /></link>
			<link block="cost"><property name="Cost" /></link>
			<link block="type" separator=" --  "><property name="Type" /><property name="Subtype" /></link>
			<link block="rules"><property name="Rules" /></link>
			<link block="ptbox"><property name="PT Box" /></link>
		</textblocks>
	</template>

	<template src="proxy/back/c.jpg" >
		<matches>
			<match name="Flags" value=""/>
		</matches>
		<overlayblocks>
			<link block="bc" />
			<conditional><if property="Color" contains="Multicolor"><link block="bm" /></if></conditional>
			<conditional><if property="Color" value="White"><link block="bw" /></if></conditional>
			<conditional><if property="Color" value="Blue"><link block="bu" /></if></conditional>
			<conditional><if property="Color" value="Black"><link block="bb" /></if></conditional>
			<conditional><if property="Color" value="Red"><link block="br" /></if></conditional>
			<conditional><if property="Color" value="Green"><link block="bg" /></if></conditional>

			<conditional><if property="Color" value="Hybrid White Blue"><link block="bw" /><link block="bhu" /></if></conditional>
			<conditional><if property="Color" value="Hybrid White Black"><link block="bw" /><link block="bhu" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Blue Black"><link block="bu" /><link block="bhb" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Blue Red"><link block="bu" /><link block="bhr" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Black Red"><link block="bb" /><link block="bhr" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Black Green"><link block="bb" /><link block="bhg" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Red Green"><link block="br" /><link block="bhg" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Red White"><link block="br" /><link block="bhw" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Green White"><link block="bg" /><link block="bhw" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Green Blue"><link block="bg" /><link block="bhu" /></if></conditional>
			<conditional><if property="Type" contains="Artifact"><link block="ba" /></if></conditional>

			<conditional><if property="Color" contains="Colorless"><link block="rc" /><link block="tc" /></if></conditional>
			<conditional><if property="Type" contains="Artifact"><link block="ra" /><link block="ta" /></if></conditional>
			<conditional><if property="Color" value="White"><link block="rw" /><link block="tw" /></if></conditional>
			<conditional><if property="Color" value="Blue"><link block="ru" /><link block="tu" /></if></conditional>
			<conditional><if property="Color" value="Black"><link block="rb" /><link block="tb" /></if></conditional>
			<conditional><if property="Color" value="Red"><link block="rr" /><link block="tr" /></if></conditional>
			<conditional><if property="Color" value="Green"><link block="rg" /><link block="tg" /></if></conditional>
			<conditional><if property="Color" contains="Multicolor"><link block="tm" /></if></conditional>
			<conditional><if property="Color" contains="Hybrid"><link block="tc" /></if></conditional>

			<conditional><if property="Color" value="Hybrid White Blue"><link block="rw" /><link block="rhu" /></if></conditional>
			<conditional><if property="Color" value="Hybrid White Black"><link block="rw" /><link block="rhu" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Blue Black"><link block="ru" /><link block="rhb" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Blue Red"><link block="ru" /><link block="rhr" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Black Red"><link block="rb" /><link block="rhr" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Black Green"><link block="rb" /><link block="rhg" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Red Green"><link block="rr" /><link block="rhg" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Red White"><link block="rr" /><link block="rhw" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Green White"><link block="rg" /><link block="rhw" /></if></conditional>
			<conditional><if property="Color" value="Hybrid Green Blue"><link block="rg" /><link block="rhu" /></if></conditional>

			<conditional><if property="Color" value="Multicolor White Blue"><link block="rw" /><link block="rhu" /></if></conditional>
			<conditional><if property="Color" value="Multicolor White Black"><link block="rw" /><link block="rhu" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Blue Black"><link block="ru" /><link block="rhb" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Blue Red"><link block="ru" /><link block="rhr" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Black Red"><link block="rb" /><link block="rhr" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Black Green"><link block="rb" /><link block="rhg" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Red Green"><link block="rr" /><link block="rhg" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Red White"><link block="rr" /><link block="rhw" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Green White"><link block="rg" /><link block="rhw" /></if></conditional>
			<conditional><if property="Color" value="Multicolor Green Blue"><link block="rg" /><link block="rhu" /></if></conditional>
		</overlayblocks>
		<textblocks>
			<link block="title"><property name="Name" /></link>
			<link block="cost"><property name="Cost" /></link>
			<link block="type" separator=" --  "><property name="Type" /><property name="Subtype" /></link>
			<link block="rules"><property name="Rules" /></link>
			<link block="ptbox"><property name="PT Box" /></link>
		</textblocks>
	</template>
